def bubble_sort(a_list):
    for j in range(len(a_list)-1,0,-1):
        for i in range(j):
            if a_list[i] > a_list[i+1]:
                temp = a_list[i]
                a_list[i]=a_list[i+1]
                a_list[i+1] = temp


a_list = [1,2,4,3,6,5,7,8,10,9]
bubble_sort(a_list)
print(a_list)
