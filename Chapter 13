f = open("test.txt", "rb")
s = f.read()
f.close()
f = open("newtext.txt", "wb")
f.write(s[::-1])
f.close()
print(f)

f = open("Snake.txt","w")
f.write("This text is used for Chapter 13.11 Exercise Qn 2: \n The Pythonidae, commonly known simply as pythons, from the Greek word \n python, are a family of nonvenomous snakes found in Africa, Asia,\n and Australia.\n Most members of this family are ambush predators, in that they typically \nremain motionless in a camouflaged position, and then strike suddenly at \n passing prey.")
f.close()
print(f)

def filter(oldfile , newfile):
    infile = open(oldfile,"r")
    outfile = open(newfile ,"w")
    while True:
        Text = infile.readline()
        if len(Text) == 0:
            break
        if Text[0] == "#":
            continue

        outfile.write()

    infile.close()
    outfile.close()
    
